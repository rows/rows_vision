# AI API Keys (at least one is required)
API_KEY_ANTHROPIC=your_anthropic_api_key_here
API_KEY_OPENAI=your_openai_api_key_here
API_KEY_GEMINI=your_gemini_api_key_here
API_KEY_GROQ=your_groq_api_key_here

# Optional: Model Configuration
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022
OPENAI_MODEL=gpt-4o
GEMINI_MODEL=gemini-2.0-flash
GROQ_MODEL=meta-llama/llama-4-scout-17b-16e-instruct

# Optional: Server Configuration
HOST=0.0.0.0
PORT=8080
DEBUG=false
LOG_LEVEL=INFO

# Optional: Processing Configuration
MAX_FILE_SIZE=10485760
MAX_RETRIES=3
REQUEST_TIMEOUT=30
MAX_TOKENS=8000

# Optional: Feature Flags
ENABLE_PDF=false
ENABLE_BATCH=false
ENABLE_TIMING=true

# Optional: Default Models
DEFAULT_CLASSIFICATION_MODEL=anthropic
DEFAULT_EXTRACTION_MODEL=anthropic